{
    "code-duplication": {
        "args": "--ignore */resources/code_duplication_failed/* --gitignore"
    },
    "dependencies-check": {
        "args": {
            "ignoreMatches": [
                "@skypack/package-check",
                "jscpd"
            ]
        }
    },
    "require-time": {
        "budget": {
            "fail": { "entrypointTime": 500000000 }
        }
    },
    "dependencies-size": {
        "budget": {
            "fail": { "depth": 9 }
        }
    },
    "updates": {
        "budget": {
            "fail": { "major": 1 },
            "warn": { "major": 1 },
            "info": { "major": 1 }
        }
    }
}