{
    "code-duplication": {
        "args": "--ignore */resources/code_duplication_failed/* --gitignore"
    },
    "dependencies-check": {
        "args": {
            "ignoreMatches": [
                "@skypack/package-check",
                "jscpd"
            ]
        }
    },
    "require-time": {
        "budget": {
            "fail": { "entrypointTime": 500000000 }
        }
    },
    "dependencies-size": {
        "budget": {
            "fail": { "depth": 8, "weight": 100000000, "dependencies": 300, "directDependencies": 10 }
        }
    }
}